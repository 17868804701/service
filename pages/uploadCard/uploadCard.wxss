@import '../style/weui.wxss';
page{
    background-color: #F8F8F8;
    font-size: 16px;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.page__hd {
    padding: 40px;
}
.page__bd {
    padding-bottom: 40px;
}
.page__bd_spacing {
    padding-left: 15px;
    padding-right: 15px;
}

.page__ft{
    padding-bottom: 10px;
    text-align: center;
}

.page__title {
    text-align: left;
    font-size: 20px;
    font-weight: 400;
}

.page__desc {
    margin-top: 5px;
    color: #888888;
    text-align: left;
    font-size: 14px;
}
.info{
  border: 0px solid #eee;
  width: 100%;
  height: 200rpx;
  font-size: 30rpx;
}
.tijiao{
  background-color: #5FB878;
  border: 0;
  margin-top: 30rpx;
  width: 90%;
  margin-left: 5%;
  color: white;
  height: 80rpx;
  line-height: 80rpx;
}
.title{
  font-size: 30rpx;
}
.num{
  font-size: 30rpx;
}
.tel input{
  border: 1px solid #eee;
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 20rpx;
}